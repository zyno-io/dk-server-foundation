        "nodemailer": "^8.0.1",
        "opentelemetry-node-metrics": "^3.0.0",
        "patch-package": "^8.0.1",
        "pino": "^10.3.1",
        "pino-pretty": "^13.1.3",
        "postmark": "^4.0.7",
        "rxjs": "~7.8.2",
        "send": "^1.2.1",
