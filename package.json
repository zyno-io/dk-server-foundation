        "uuidv7": "^1.1.0",
        "ws": "^8.19.0",
        "yaml": "^2.8.2"
    },
    "devDependencies": {
        "@types/bcrypt": "^6.0.0",
        "@types/debug": "^4.1.12",
        "@types/google-libphonenumber": "^7.4.30",
        "@types/lodash": "^4.17.23",
        "@types/node": "^25.3.0",
        "@types/nodemailer": "^7",
        "lefthook": "^2.1.1",
        "npm-run-all": "^4.1.5",
        "oxlint": "^1.49.0",
        "prettier": "^3.8.1",
        "ts-essentials": "^10.1.1",
        "ts-node": "^10.9.2",
        "tslib": "^2.8.1",
        "type-fest": "^5.4.4",
        "typescript": "~5.9"
    },
    "files": [
        "dist/src/",
        "dist/devconsole/",
        "dist/resources/",
        "docs/",
        "patches/",
        "src/",
        "types.d.ts",
        "!**/*.tsbuildinfo",
        "!**/CLAUDE.md",
        "!**/*_MIGRATION_GUIDE.md"
    ],
    "main": "./dist/src/index.js",
    "types": "./dist/src/index.d.ts",
    "exports": {
        ".": "./dist/src/index.js",
        "./*": "./dist/src/*"
    },
    "packageManager": "yarn@4.12.0"
}
